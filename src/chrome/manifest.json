{
  "name": "Tweet Verifier",
  "version": "0.1",
  "description": "Check if Tweets are real",
  "browser_action": {
    "default_icon": {
      "16": "assets/tweet-verifier16.png",
      "32": "assets/tweet-verifier32.png",
      "48": "assets/tweet-verifier48.png",
      "128": "assets/tweet-verifier128.png"
    }
  },
  "permissions": ["activeTab"],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "assets/tweet-verifier16.png",
    "32": "assets/tweet-verifier32.png",
    "48": "assets/tweet-verifier48.png",
    "128": "assets/tweet-verifier128.png"
  },
  "manifest_version": 2
}